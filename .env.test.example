# Firebase Configuration for E2E Tests
# Copy this to .env.test and fill in your values
# Uses local emulators for isolated testing

VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:placeholder

# Environment
VITE_APP_ENV=test

# Enable emulators for E2E tests
VITE_USE_EMULATORS=true

# Emulator URLs
VITE_EMULATOR_AUTH_URL=http://localhost:9099
VITE_EMULATOR_FIRESTORE_HOST=localhost:8080
VITE_EMULATOR_FUNCTIONS_URL=http://localhost:5001
